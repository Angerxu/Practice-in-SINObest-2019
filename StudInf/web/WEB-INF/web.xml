<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>deleteStudInf</servlet-name>
        <servlet-class>system.delete.deleteStudInf</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>deleteStudInf</servlet-name>
        <url-pattern>/system/delete/deleteStudInf</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>insertStudInf</servlet-name>
        <servlet-class>system.insert.insertStudInf</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>insertStudInf</servlet-name>
        <url-pattern>/system/insert/insertStudInf</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>readStudInf</servlet-name>
        <servlet-class>system.read.readStudInf</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>readStudInf</servlet-name>
        <url-pattern>/system/read/readStudInf</url-pattern>
    </servlet-mapping>
</web-app>